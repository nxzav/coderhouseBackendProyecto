<div class="main-content">
  {{#each carts}}
    <div class="cart">
      <a class="cart-link" href="/carts/{{_id}}"><h1>Cart: {{_id}}</h1></a>
      {{#each products}}
        <div>
          {{#each product}}
          <div>
            Product: {{title}}
            <br>
            Description: {{description}}
            <br>
            Thumbnail: {{thumbnails}}
          {{/each}}
          <p>Quantity: {{quantity}}</p>
          </div>
        </div>
        <hr>
      {{/each}}
    </div>
  {{/each}}
</div>